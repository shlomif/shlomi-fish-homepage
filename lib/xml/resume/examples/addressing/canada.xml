  <header>
    <name>
      <firstname>M.J.</firstname>
      <surname>Arthur</surname>
    </name>
    <address>
      <street>1165 Rue McLean</street>
      <street2>Suite 1500</street2>
      <city>Kamloops</city>
      <province>BC</province>
