<define-tag paypal_donate_why_donate_link>
<a href="<rellink host="t2" url="meta/donate/" />">Why Donate?</a>
</define-tag>
<define-tag paypal_donate_code>
<preserve type />
<set-var %attributes />
<form action="https://www.paypal.com/cgi-bin/webscr" method="post">
<div class="search">
<input type="hidden" name="cmd" value="_s-xclick" />
<input type="image" src="$(ROOT)/images/paypal.gif" style="border:0" name="submit" alt="Make payments with PayPal - it's fast, free and secure!" />
<ifeq "<get-var type />" "nolink" "" "<paypal_donate_why_donate_link />" />
<input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----
" />
</div>
</form>
<restore type />
</define-tag>

